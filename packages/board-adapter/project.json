{
  "name": "board-adapter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/board-adapter/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "packages/board-adapter"
      }
    },
    "move-package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../scripts/move-package.mjs",
        "cwd": "packages/board-adapter"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "packages/board-adapter"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "packages/board-adapter/dist"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "math",
    "being"
  ]
}
