{
  "name": "curve",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/curve/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/curve"
      }
    },
    "build:bun": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/curve"
      }
    },
    "move-package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../scripts/move-package.mjs",
        "cwd": "packages/curve"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test",
        "cwd": "packages/curve"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "packages/curve/dist",
        "publishCommand": "npm publish"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "math"
  ]
}
