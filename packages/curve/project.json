{
  "name": "curve",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/curve/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/curve"
      }
    },
    "build:bun": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/curve"
      }
    },
    "move-package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../scripts/move-package.mjs",
        "cwd": "packages/curve"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test",
        "cwd": "packages/curve"
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../../scripts/publish-package.mjs",
        "forwardAllArgs": false,
        "cwd": "packages/curve/dist"
      }
    },
    "docs:build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf docs/curve && bun run typedoc --skipErrorChecking --entryPoints packages/curve/src/index.ts --out docs/curve"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "math"
  ]
}
