{
  "name": "animate",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/animate/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/animate"
      }
    },
    "build:bun": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/animate"
      }
    },
    "move-package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../scripts/move-package.mjs",
        "cwd": "packages/animate"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test",
        "cwd": "packages/animate"
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --access public --registry https://registry.npmjs.org --no-git-checks",
        "cwd": "packages/animate/dist"
      }
    },
    "docs:build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf docs/animate && bun run typedoc --entryPoints packages/animate/src/index.ts --out docs/animate"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "math"
  ]
}
