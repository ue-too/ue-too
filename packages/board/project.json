{
  "name": "board",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/board/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "packages/board"
      }
    },
    "build:bun": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build.ts",
        "cwd": "packages/board"
      }
    },
    "move-package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../scripts/move-package.mjs",
        "cwd": "packages/board"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "packages/board"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "packages/board/dist"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "math",
    "being"
  ]
}
