{
  "name": "ue-too",
  "description": "pan, zoom, and rotate your html canvas",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "nx run-many --target=test",
    "dev:banana": "nx dev banana",
    "build": "nx run-many --target=build --projects=packages/* --parallel=true",
    "build:packages": "nx run-many --target=build --projects=packages/* --parallel=true",
    "build:apps": "nx run-many --target=build --projects=apps/* --parallel=true",
    "build:affected": "nx affected --target=build --parallel=true",
    "build:examples": "nx build examples",
    "build:banana": "nx build banana",
    "bump-version": "nx release --skip-publish",
    "bump-version:dry-run": "nx release --skip-publish --dry-run",
    "move-packageJson": "nx run-many --target=move-package --parallel=true",
    "publish-packages": "nx release publish",
    "changeset": "changeset",
    "version-packages": "changeset version && bun run build && bun run replace-workspace-deps",
    "release": "changeset publish",
    "replace-workspace-deps": "node scripts/replace-workspace-deps.mjs",
    "dev:examples": "nx dev examples",
    "scaffold:react": "node scripts/scaffold-react.mjs",
    "scaffold:vue": "node scripts/scaffold-vue.mjs",
    "scaffold:package": "bun scripts/scaffold-package.mjs",
    "docs:build": "nx run-many --target=docs:build --projects=packages/* --parallel=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ue-too/ue-too.git"
  },
  "author": "niuee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ue-too/ue-too/issues"
  },
  "homepage": "https://github.com/ue-too/ue-too#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@nx/devkit": "22.2.0",
    "@nx/jest": "22.2.0",
    "@nx/js": "22.2.0",
    "@nx/playwright": "22.2.0",
    "@nx/react": "^22.2.0",
    "@nx/rollup": "^21.6.9",
    "@nx/vite": "22.2.0",
    "@nx/vue": "^22.2.0",
    "@nx/web": "22.2.0",
    "@playwright/test": "^1.36.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.3",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/bun": "^1.3.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^30.0.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "^22.2.0",
    "prettier": "^2.6.2",
    "rollup": "^4.53.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.0",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": "22.19.0"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "6.29.0",
    "vue": "^3.5.13",
    "zustand": "^5.0.9"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
