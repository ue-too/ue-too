{
  "name": "ue-too",
  "description": "pan, zoom, and rotate your html canvas",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "nx run-many --target=test",
    "build": "nx run-many --target=build --projects=math,being,animate,border,curve,dynamics,board --parallel=false",
    "build:packages": "nx run-many --target=build --projects=packages/* --parallel=true",
    "build:apps": "nx run-many --target=build --projects=apps/* --parallel=true",
    "build:affected": "nx affected --target=build --parallel=true",
    "bump-version": "nx release --skip-publish",
    "bump-version:dry-run": "nx release --skip-publish --dry-run",
    "move-packageJson": "nx run-many --target=move-package --parallel=true",
    "publish-packages": "nx release publish",
    "changeset": "changeset",
    "version-packages": "changeset version && pnpm build && pnpm replace-workspace-deps",
    "release": "changeset publish",
    "replace-workspace-deps": "node scripts/replace-workspace-deps.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ue-too/ue-too.git"
  },
  "author": "niuee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ue-too/ue-too/issues"
  },
  "homepage": "https://github.com/ue-too/ue-too#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@niuee/bounce": "^0.0.5",
    "@niuee/typedoc-plugin-translation": "^0.0.5",
    "@nx/js": "^21.3.5",
    "@nx/rollup": "^21.3.5",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "fabric": "^6.7.0",
    "jest": "^29.7.0",
    "konva": "^9.3.20",
    "nx": "21.3.5",
    "pixi.js": "^8.8.0",
    "rollup": "^4.34.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tslib": "^2.8.1",
    "typedoc": "0.27.7",
    "typedoc-plugin-localization": "^3.0.6",
    "typedoc-plugin-markdown": "^4.4.2",
    "typedoc-plugin-mdn-links": "^4.0.13",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
}
