{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "// targets": "to see all targets run: nx show project board-react --web",
    "name": "board-react",
    "projectType": "application",
    "sourceRoot": "apps/board-react/src",
    "tags": [],
    "targets": {
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite",
                "cwd": "apps/board-react"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun test",
                "cwd": "apps/board-react"
            }
        }
    }
}
