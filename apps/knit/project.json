{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "name": "knit",
    "projectType": "application",
    "sourceRoot": "apps/knit/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun run build",
                "cwd": "apps/knit"
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite",
                "cwd": "apps/knit"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun test",
                "cwd": "apps/knit"
            }
        }
    }
}
